{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@pages/*": [
        "cypress/pages/*"
      ],
      "@cypress/grep/plugin": [
        "node_modules/@cypress/grep/dist/plugin"
      ],
      "@cypress/grep": [
        "node_modules/@cypress/grep/dist/index"
      ]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "node_modules/@cypress/grep/dist/index.d.ts"
  ]
}